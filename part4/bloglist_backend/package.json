{
  "name": "phonebook_backend",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "start": "node index.js",
    "dev": "NODE_ENV=development nodemon index.js",
    "test": "jest --verbose",
    "build:ui": "rm -rf dist && cd ../phonebook_frontend && npm run build && cp -r dist ../phonebook_backend",
    "deploy:full": "npm run build:ui && git add . && git commit -m uibuild",
    "lint": "eslint .",
    "prettier": "prettier -w . '!dist'"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "lodash": "^4.17.21",
    "mongoose": "^7.3.2",
    "morgan": "^1.10.0"
  },
  "devDependencies": {
    "eslint": "^8.44.0",
    "jest": "^29.6.1",
    "nodemon": "^3.0.1",
    "prettier": "^2.8.7"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "prettier": {
    "singleQuote": true
  }
}
